{
	"name": "gtt-user-management-ms",
	"version": "0.0.1",
	"description": "This is the User management Microservice",
	"main": "build/index.js",
	"module": "src/index.js",
	"requiredVariables": [
		"SAMPLE"
	],
	"scripts": {
		"build": "babel src --out-dir build",
		"debug": "nodemon --inspect --exec babel-node src/index.js",
		"dev": "nodemon --exec babel-node src/index.js --ignore jsconfig.json",
		"start": "node build/index.js",
		"format": "prettier --write \"**/*.{js,mjs,cjs}\"",
		"format:check": "prettier -c \"**/*.{js,mjs,cjs}\"",
		"lint": "eslint \"src/**/*.{js,mjs,cjs}\"",
		"lint:fix": "npm run lint -- --fix",
		"test": "mocha --require @babel/register --timeout 10000",
		"test-with-coverage": "nyc --reporter=html npm test --reporter=lcov"
	},
	"keywords": [],
	"author": "Tech Mahindra-GTT",
	"license": "ISC",
	"dependencies": {
		"@babel/runtime": "^7.15.4",
		"babel-node": "^0.0.1-security",
		"babel-plugin-module-resolver": "^4.1.0",
		"chalk": "^4.1.1",
		"config": "^3.3.6",
		"cors": "^2.8.5",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"express-async-errors": "^3.1.1",
		"express-rate-limit": "^5.2.6",
		"express-validation": "^3.0.8",
		"helmet": "^4.6.0",
		"http-status-codes": "^2.1.4",
		"ioredis": "^4.28.0",
		"joi": "^17.4.2",
		"joi-objectid": "^4.0.2",
		"mongoose": "^6.0.11",
		"morgan": "^1.10.0",
		"morgan-body": "^2.6.6",
		"pm2": "^5.1.2",
		"save": "^2.4.0",
		"swagger-ui-express": "^4.1.6",
		"winston": "^3.3.3",
		"winston-daily-rotate-file": "^4.5.5"
	},
	"devDependencies": {
		"@babel/cli": "^7.14.5",
		"@babel/core": "^7.14.6",
		"@babel/node": "^7.14.5",
		"@babel/plugin-proposal-throw-expressions": "^7.14.5",
		"@babel/plugin-transform-runtime": "^7.14.5",
		"@babel/preset-env": "^7.14.5",
		"@babel/register": "^7.16.0",
		"@commitlint/cli": "^13.1.0",
		"@commitlint/config-conventional": "^13.1.0",
		"@trivago/prettier-plugin-sort-imports": "^2.0.2",
		"@types/jest": "^26.0.23",
		"babel-plugin-istanbul": "^6.1.1",
		"chai": "^4.3.4",
		"chai-http": "^4.3.0",
		"cross-env": "^7.0.3",
		"eslint": "^7.28.0",
		"eslint-plugin-jest": "^24.3.6",
		"husky": "^7.0.1",
		"istanbul": "^0.4.5",
		"mocha": "^9.1.3",
		"nodemon": "^2.0.7",
		"nyc": "^15.1.0",
		"prettier": "^2.3.1",
		"pretty-quick": "^3.1.1"
	}
}
